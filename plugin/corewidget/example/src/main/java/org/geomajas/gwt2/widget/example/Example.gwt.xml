<module rename-to="example">
	<inherits name='org.geomajas.gwt2.widget.example.ExampleJar' />
    <entry-point class="org.geomajas.gwt2.widget.example.client.Example" />

	<!-- set target browser to compile for, use this to limit to the browser used for testing -->
	<!-- where value = "ie6/opera/gecko1_8/safari/gecko" , "gecko1_8" is FireFox 3 -->
	<set-property name="user.agent" value="gecko1_8" />

	<replace-with class="org.geomajas.gwt2.widget.client.nostyle.CoreWidgetClientBundleFactoryNoStyle">
    <when-type-is class="org.geomajas.gwt2.widget.client.CoreWidgetClientBundleFactory"/>
  </replace-with>
	<source path='client' />
</module>